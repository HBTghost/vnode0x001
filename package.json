{
  "name": "vnode0x001",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "14.x"
  },
  "description": "Project 1 - full stack with Vue.js + Node.js",
  "main": "server.js",
  "scripts": {
    "start": "cd server && node server",
    "server": "cd server && nodemon --exec babel-node server",
    "client": "cd client && yarn serve",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "prod": "cd client && yarn build && cd .."
  },
  "repository": "https://github.com/HBTghost/vnode0x001.git",
  "author": "Brian Drake (HBTghost)",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
